{
    "name": "idez/nova-date-range-filter",
    "description": "A Laravel Nova date range filter.",
    "keywords": [
        "laravel",
        "nova",
        "date",
        "range",
        "filter",
        "idez"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Laurindo",
            "email": "rafael@idez.com.br",
            "homepage": "https://idez.com.br",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/idezdigital/nova-date-range-filter/issues"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0 || ^10.0 || ^11.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Idez\\DateRangeFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Idez\\DateRangeFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
